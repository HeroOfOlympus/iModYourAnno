<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Styleguide: 
            Components that display something -> ComponentColorBrush
            Components that are clickable -> InteractiveComponentColorBrush
        -->
    <SolidColorBrush x:Key="ComponentColorBrush">#C0C0C0</SolidColorBrush>
    <SolidColorBrush x:Key="ComponentColorBrush_Light">#C5C5C5</SolidColorBrush>

    <SolidColorBrush x:Key="InteractiveComponentColorBrush">#B0B0B0</SolidColorBrush>
    <SolidColorBrush x:Key="InteractiveComponentColorBrush_Light">#B5B5B5</SolidColorBrush>
    <SolidColorBrush x:Key="InteractiveComponentColorBrush_Dark">#909090</SolidColorBrush>

    <SolidColorBrush x:Key="ComponentColorBrush_Pressed">DarkGray</SolidColorBrush>
    <SolidColorBrush x:Key="ComponentColorBrush_Inactive">#F5F5F5</SolidColorBrush>

    <SolidColorBrush x:Key="AccentColorBrush">#bdae51</SolidColorBrush>
    <SolidColorBrush x:Key="AccentColorBrush_Dark">#f7eb99</SolidColorBrush>

    <SolidColorBrush x:Key="BackgroundColorBrush">LightGray</SolidColorBrush>
    <SolidColorBrush x:Key="TextColorBrush">#120A24</SolidColorBrush>

    <SolidColorBrush x:Key="ModActiveColorBrush">#120A24</SolidColorBrush>
    <SolidColorBrush x:Key="ModInactiveColorBrush">#2E0E18</SolidColorBrush>
</ResourceDictionary>